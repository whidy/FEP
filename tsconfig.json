{
  "compilerOptions": {
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "module": "esnext",
    "target": "esnext",
    "allowJs": false,
    "noUnusedLocals": false,
    "preserveConstEnums": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "@formily/element-plus": [
        "packages/components/src"
      ],
      "@formily/element-plus-*": [
        "packages/*/src"
      ],
    },
    "types": [
      "vite/client"
    ]
  }
}
